<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" href="/stylesheets/header.css">
    <link rel="stylesheet" href="/stylesheets/footer.css">
    <link rel="stylesheet" href="/stylesheets/gigs.css">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css">
    <title>Document</title>
</head>
<body>
    <%- include("partials/header.ejs") %> 
    <form action="/gig/find" method="POST">

    <section class="gigs-search">
        <input type="text" placeholder="I'm looking for" name="searchQuery" />
        <button>Search</button>
    </section>
</form>

    <section class="gigs-container">
        <% for(let i=0;i<gigs.length ;i++) {%> 
        <div style="width: 100%;">
            <a class="up-orders-gig-a" href="/gig/view/<%= gigs[i].id %> ">
                <div class="up-orders-gig">
                    <img class="up-orders-gig-image" src="<%= gigs[i].p_url %> " alt="">
                    <div class="up-gig-desc">
                        <div class="up-gig-profile">
                            <% if (gigs[i].img_url == null) { %>
                                <img src="data:image/png;base64," alt="">
                            <% } else{%>
                                <img src="<%= gigs[i].img_url %> " alt="">
                            <% } %> 
                            <h4><a style="text-decoration: none; color: black;" href="/user/freelacer/profile/<%= gigs[i].Uid %> "><%= gigs[i].username %></a></h4> </h4>
                        </div>
                        <p style="flex-grow: 1;"><%= gigs[i].title %> </p>
                    </div>
                    <hr color="lightgray">
                    <h3><span>Price </span> lkr. <%= gigs[i].price %> </h3>
                </div>
            </a>
        </div>
        <% } %>
    </section>
    <section class="gigs-pagination">
        <% for(let i=0;i < 10; i++ ) {%> 
            <div class="gigs-pagi">
                <%= i %> 
            </div>
        <% } %> 
        <div class="gigs-pagi">
            >
        </div>
    </section>
    <%- include("partials/footer.ejs") %> 
    <script  src="/javascript/header.js"></script>
    <script  src="/javascript/search.js"></script>
    <script  src="/javascript/home.js"></script>
<body>
</html>